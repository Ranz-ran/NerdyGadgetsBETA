<+>* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\n.container {\r\n    width: 15%;\r\n    min-height: 15%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background: #C0C0C0;\r\n}\r\n\r\n.menu {\r\n    position: relative;\r\n    width: 170px;\r\n    height: 170px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n\r\n.menu .navigation {\r\n    position: relative;\r\n    width: 40px;\r\n    height: 40px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    cursor: pointer;\r\n    transition: 0.5s;\r\n}\r\n\r\n.menu .navigation span {\r\n    position: absolute;\r\n    width: 7px;\r\n    height: 7px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background: #fff;\r\n    transform: translate(calc(14px * var(--x)), calc(14px * var(--y)));\r\n    transition: transform 0.5s, width 0.5s, height 0.5s;\r\n    transition-delay: calc(0.1s * var(--i));\r\n}\r\n\r\n.menu .navigation span ion-icon {\r\n    color: #fff;\r\n    font-size: 0.25rem;\r\n    transition: 0.5s;\r\n}\r\n\r\n.menu .navigation.active span ion-icon {\r\n    font-size: 1.75rem;\r\n    transition: 0.5s;\r\n}\r\n\r\n.menu .navigation.active span:hover ion-icon {\r\n    color: yellow;\r\n    filter: drop-shadow(0 0 3px yellow) drop-shadow(0 0 6px yellow) drop-shadow(0 0 12px yellow);\r\n}\r\n\r\n.menu .navigation.active span {\r\n    width: 45px;\r\n    height: 45px;\r\n    background: #1f2536;\r\n    transform: translate(calc(60px * var(--x)), calc(60px * var(--y)));\r\n}\r\n\r\n.menu .close {\r\n    position: absolute;\r\n    width: 7px;\r\n    height: 7px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background: #fff;\r\n    transition: 0.5s;\r\n    cursor: pointer;\r\n}\r\n\r\n.menu .navigation.active ~ .close {\r\n    width: 40px;\r\n    height: 40px;\r\n    pointer-events: initial;\r\n    transition-delay: 0.8s;\r\n    background: yellow;\r\n    color: #fff;\r\n}\r\n\r\n.menu .navigation ~ .close ion-icon {\r\n    color: #10131c;\r\n    font-size: 2.5rem;\r\n    scale: 0;\r\n    transition: 1s;\r\n}\r\n\r\n.menu .navigation.active ~ .close ion-icon {\r\n    scale: 1;\r\n    transition-delay: 0.5s;\r\n}
